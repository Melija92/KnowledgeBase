<span style='color:#800080; '>{</span>
    <span style='color:#800000; font-weight:bold; '>this</span><span style='color:#808030; '>.</span>tableItems <span style='color:#808030; '>=</span> myItems<span style='color:#800080; '>;</span>
    sections <span style='color:#808030; '>=</span> <span style='color:#808030; '>(</span>from section <span style='color:#800000; font-weight:bold; '>in</span> tableItems
               where <span style='color:#808030; '>!</span>String<span style='color:#808030; '>.</span>IsNullOrEmpty <span style='color:#808030; '>(</span>section<span style='color:#808030; '>)</span>
               select section<span style='color:#808030; '>.</span>Substring <span style='color:#808030; '>(</span><span style='color:#008c00; '>0</span><span style='color:#808030; '>,</span> <span style='color:#008c00; '>1</span><span style='color:#808030; '>)</span><span style='color:#808030; '>)</span><span style='color:#808030; '>.</span>Distinct <span style='color:#808030; '>(</span><span style='color:#808030; '>)</span><span style='color:#808030; '>.</span>ToList <span style='color:#808030; '>(</span><span style='color:#808030; '>)</span><span style='color:#800080; '>;</span>
 
    <span style='color:#800000; font-weight:bold; '>foreach</span> <span style='color:#808030; '>(</span><span style='color:#800000; font-weight:bold; '>var</span> item <span style='color:#800000; font-weight:bold; '>in</span> tableItems<span style='color:#808030; '>)</span> <span style='color:#800080; '>{</span>
    <span style='color:#800000; font-weight:bold; '>int</span> sectionNumber <span style='color:#808030; '>=</span> sections<span style='color:#808030; '>.</span>IndexOf <span style='color:#808030; '>(</span>item<span style='color:#808030; '>.</span>ToString <span style='color:#808030; '>(</span><span style='color:#808030; '>)</span><span style='color:#808030; '>.</span>Substring <span style='color:#808030; '>(</span><span style='color:#008c00; '>0</span><span style='color:#808030; '>,</span> <span style='color:#008c00; '>1</span><span style='color:#808030; '>)</span><span style='color:#808030; '>)</span><span style='color:#800080; '>;</span>
