            sectionData <span style='color:#808030; '>[</span>sectionNumber<span style='color:#808030; '>]</span><span style='color:#808030; '>.</span>Add <span style='color:#808030; '>(</span>item<span style='color:#808030; '>)</span><span style='color:#800080; '>;</span>
        <span style='color:#800080; '>}</span> <span style='color:#800000; font-weight:bold; '>else</span> <span style='color:#800080; '>{</span> 
            sectionData<span style='color:#808030; '>.</span>Add <span style='color:#808030; '>(</span>sectionNumber<span style='color:#808030; '>,</span> <span style='color:#800000; font-weight:bold; '>new</span> List<span style='color:#808030; '>&lt;</span><span style='color:#800000; font-weight:bold; '>string</span><span style='color:#808030; '>></span> <span style='color:#800080; '>{</span> item <span style='color:#800080; '>}</span><span style='color:#808030; '>)</span><span style='color:#800080; '>;</span>
        <span style='color:#800080; '>}</span>
    <span style='color:#800080; '>}</span>
